              "pm.expect(json.Response).to.exist;",
              "pm.expect(json.Response.ResponseStatus).to.eql(1);",
              "pm.expect(json.Response.Results && json.Response.Results.length > 0).to.be.true;",
              "if (json.Response.Results.length > 0) {",
              "  pm.collectionVariables.set('RESULT_INDEX', json.Response.Results[0][0].ResultIndex);",
              "  pm.collectionVariables.set('SESSION_ID', json.SessionId);",
              "}"
            ],
            "type": "text/javascript"
          }
        }
      ],
