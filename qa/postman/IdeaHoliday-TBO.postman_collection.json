  "item": [
    {
      "name": "Authenticate",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "url": "{{TBO_BASE}}/SharedServices/SharedData.svc/rest/Authenticate",
        "body": {
          "mode": "raw",
          "raw": "{\"ClientId\":\"{{TBO_CLIENT_ID}}\",\"UserName\":\"{{TBO_USERNAME}}\",\"Password\":\"{{TBO_PASSWORD}}\",\"EndUserIp\":\"{{END_USER_IP}}\"}"
        }
      },
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test('Status 200', () => pm.response.code === 200);",
              "const json = pm.response.json();",
              "pm.expect(json.TokenId).to.exist;",
              "pm.collectionVariables.set('TOKEN', json.TokenId);"
            ],
            "type": "text/javascript"
          }
        }
      ]
    },
    {
